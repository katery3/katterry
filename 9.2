with open('F:/Шинкаренко Екатерина_УБ-21_vvod.txt') as file:
    s = file.read()
n = int(s)
mat = [[0] * n for i in range(n)]
st, m = 1, 0
mat[n // 2][n // 2] = n * n
for v in range(n // 2):
    for i in range(n - m):
        mat[v][i + v] = st
        st += 1
    for i in range(v + 1, n - v):
        mat[i][-v - 1] = st
        st += 1
    for i in range(v + 1, n - v):
        mat[-v - 1][-i - 1] = st
        st += 1
    for i in range(v + 1, n - (v + 1)):
        mat[-i - 1][v] = st
        st += 1
    m += 2
with open('F:/Шинкаренко Екатерина_УБ-21_vivod.txt', 'w') as out:
    for i in mat:
        print(*i)
        out.write(' '.join([str(mat) for mat in i]) + '\n')
